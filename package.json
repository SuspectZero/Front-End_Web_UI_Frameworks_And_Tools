{
  "name": "assignment1",
  "version": "1.0.0",
  "description": "This is the result of completing the Front-End Web UI Frameworks and Tools course on Coursera under the Full Stack Web Development specialization.",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts && copyfiles -f node_modules/bootstrap/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "uglify": "uglifyjs dist/js/main.js -m -c -o dist/js/main.js",
    "build": "npm run clean && npm run copyfonts && npm run imagemin && npm run usemin && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuspectZero/Front-End_Web_UI_Frameworks_And_Tools.git"
  },
  "author": "Tahir Awan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SuspectZero/Front-End_Web_UI_Frameworks_And_Tools/issues"
  },
  "homepage": "https://github.com/SuspectZero/Front-End_Web_UI_Frameworks_And_Tools#readme",
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.13",
    "copyfiles": "^1.2.0",
    "cssmin": "^0.4.3",
    "del": "^3.0.0",
    "font-awesome": "^4.7.0",
    "grunt": "^1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^3.2.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^2.0.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-flatmap": "^1.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^4.0.0",
    "gulp-rev": "^8.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-usemin": "^0.3.28",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^3.0.0",
    "jit-grunt": "^0.10.0",
